{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.36",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.4.4",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "linebreak-style": "off",
      "indent": [
        "warn",
        4
      ],
      "react/jsx-indent": [
        "warn",
        4
      ],
      "react/jsx-indent-props": [
        2,
        4
      ],
      "jsx-quotes": [
        2,
        "prefer-double"
      ],
      "quotes": [
        2,
        "double"
      ],
      "no-plusplus": "off",
      "max-len": "warn",
      "react/forbid-prop-types": "warn",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "react/jsx-props-no-spreading": [
        1
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".tsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
